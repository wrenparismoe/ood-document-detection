{"image_dir": "data/RVL_CDIP/images/imagesn/n/d/o/ndo58e00/2021659869.tif", "label": "1", "words": ["<", "\\", "onte:", "__une", "13,", "1308", "U", "\u2014", "Ao", "MANUSCRIPT", "TITLE:", "SODIUM", "ENTRAINMENT", "AS", "A", "MAJOR", "MECHANISM", "OF", "SMOKE", "TRANSFER", "FROM", "THE", "2R1", "CIGARETTE", "AUTHOR(S):", "R.", "W.", "Jenkins,", "Jr.,", "R.", "T.", "Bass", "and", "D.", "D.", "McRae,", "Philip", "Morris", "Research", "Center", "and", "J.", "A.", "Brenizer,", "T.", "G.", "Williamson,", "R.", "Johnson", "and", "3.", "Hilko,", "Department", "of", "Nuclear", "Engineering,", "University", "of", "Virginia,", "Charlottesville,", "VA.", "__", "FOR", "PUBLICATION", "(journal/book", "title]:", "and/oz", "X_", "PRESENTATION", "[meeting,", "location,", "date):", "TCRC,", "Lexington,", "KY.,", "Oct.", "2-5,", "1988.", "CONFERENCE/PUBLICATION", "DEADLINE", "FOR:", "ABSTRACT", "__6/24/88", "MANUSCRIPT", "oces", "2", "eroosss", "o", "mews", "_s", "JR\u2014", "S", "\u2014", "QQQL&/", "ABSTRACT:", "In", "an", "earlier", "paper,", "Jenkins,", "et", "al.", "(1)", "reported", "on", "a", "study", "of", "the", "variability", "of", "selected", "inorganics", "in", "mainstream", "cigarette", "smoke", "as", "a", "function", "of", "aerodynamic", "particle", "size.", "Circumstantial", "evidence", "led", "to", "a", "hypothesis", "of", "mechanical", "entrainment", "of", "the", "inorganic", "residue", "as", "a", "major", "transfer", "mechanism.", "Jenkins,", "et", "al.", "(2)", "later", "reported", "on", "a", "study", "of", "the", "butt", "filtration", "of", "inorganics", "and", "further", "evidence", "was", "presented", "to", "support", "the", "entrainment", "hypothesis.", "This", "report", "discusses", "the", "direct", "observation", "of", "entrained", "sodium", "transferred", "into", "whole", "cigarette", "smoke.", "Reference", "cigarettes", "were", "made", "from", "tobacco", "labelled", "with", "*NaCl", "and", "then", "smoked", "on", "a", "total", "recovery", "smoking", "machine.", "The", "mainstream", "and", "sidestream", "particulate", "phases", "were", "collected", "on", "Cambridge", "filters.", "Autoradiographs", "of", "the", "filters", "showed", "discrete", "\"hot", "spots\u201d", "from", "particles", "labelled", "with", "the", "radioisotope.", "These", "\"spots\"", "show", "a", "non-uniform", "distribution", "on", "the", "filters", "when", "compared", "to", "the", "total", "smoke", "distribution.", "Macrophotographs", "of", "the", "pads", "showed", "discrete", "particles", "of", "inorganic", "residue", "which", "were", "directly", "correlated", "to", "the", "autoradiographs", "verifying", "the", "proposed", "entrainment", "theory.", "(1)", "Jenkins,", "R.", "W.,", "Jr.,", "B.", "W.", "Francis,", "H.", "Flachsbart,", "W.", "Stober,", "J.", "R.", "Tucci", "and", "T.", "G.", "Williamson,", "J.", "Aerosol", "Sci.", "13,", "459", "(1982).", "(2)", "Jenkins,", "R.", "W.,", "Jr.,", "C.", "Goldey", "and", "T.", "G.", "Williamson,", "Beit.", "Tabakforsch.", "13,", "59", "(1989)", ".", "PATENT", "STATUS:", "__", "Applied", "For", "__", "patented", "__", "Not", "Filed", "X", "No", "Interest", "AP", "Coordinators", "_@Mc", "coL/4-84", "INTERNAL", "REFEREES:", "6986591202"], "bbox": [[718, 60, 754, 90], [792, 49, 803, 71], [472, 158, 510, 166], [580, 159, 626, 166], [621, 137, 647, 177], [660, 158, 696, 167], [248, 188, 589, 197], [599, 189, 647, 197], [732, 182, 836, 203], [79, 219, 177, 227], [189, 219, 245, 227], [268, 219, 325, 228], [337, 220, 441, 228], [451, 220, 469, 229], [481, 221, 490, 228], [500, 221, 548, 229], [558, 221, 647, 229], [657, 222, 675, 230], [687, 222, 734, 230], [745, 222, 823, 230], [267, 235, 306, 243], [316, 235, 344, 244], [355, 235, 382, 244], [393, 235, 480, 244], [78, 265, 175, 275], [198, 266, 213, 274], [227, 266, 244, 274], [257, 265, 332, 276], [344, 267, 379, 276], [393, 267, 409, 275], [423, 267, 438, 275], [451, 267, 489, 276], [500, 267, 528, 276], [539, 268, 554, 276], [567, 268, 584, 276], [597, 268, 65