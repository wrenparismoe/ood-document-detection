{"image_dir": "data/RVL_CDIP/images/imageso/o/f/t/oft23e00/2051027782_2051027784.tif", "label": "15", "words": ["To:", "R.", "N.", "Perguson", "From:", "David", "Douglas", "and", "Frank", "Greene", "Date:", "November", "23,", "1988", "Subject:", "Analysis", "of", "Premier", "Cigarettes-III.", "Analysis", "of", "Extract", "of", "Whole", "Cigarettes", "The", "following", "work", "presents", "gc-ms", "identifications", "obtained", "by", "Eractionation", "of", "an", "extract", "of", "Premier", "Regular", "cigarettes", "sample", "Preparation", "and", "Analysis:", "Ten", "Premier", "Regular", "cigarettes", "were", "extracted", "with", "100", "ml", "of", "methanol", "by", "placing", "them", "in", "an", "ultrasonic", "bath", "for", "2", "hours,", "allowing", "them", "to", "extract", "overnight,", "and", "repeating", "the", "ultrasonic", "bath", "treatment.", "The", "methanolic", "extract", "was", "decanted", "and", "concentrated", "by", "boiling", "the", "methanol", "away", "using", "a", "Snyder", "condenser", "over", "a", "50", "ml", "centrifuge", "tube", "placed", "in", "a", "boiling", "water", "bath.", "Loss", "of", "the", "extracted", "volatiles", "was", "prevented", "by", "adding", "heptane,", "which", "gradually", "displaced", "the", "methanol.", "The", "heptane", "solution", "was", "chromatographed", "through", "a", "20", "cm", "X", "2", "cm", "diameter", "column", "of", "35-70", "mesh", "silica", "gel", "with", "a", "total", "of", "20", "ml", "heptane.", "The", "residue", "was", "subsequently", "washed", "from", "the", "centrifuge", "tube", "onto", "the", "top", "of", "the", "silica", "gel", "column", "with", "ethyl", "acetate,", "and", "eluted", "with", "a", "total", "of", "20", "ml.", "A", "similar", "procedure", "followed", "using", "methanol", "as", "the", "most", "polar", "solvent.", "The", "fractions", "eluted", "by", "each", "solvent", "were", "concentrated", "by", "boiling", "to", "a", "volume", "of", "1", "ml.", "Five", "microliters", "of", "the", "concentrated", "eluates", "were", "analyzed", "by", "capillary", "gc-ms", "using", "previously", "described", "conditions", "(1).", "Results:", "The", "heptane", "fraction", "contained", "waxes", "as", "expected.", "Specific", "identifications", "were", "not", "done.", "The", "methanol", "fraction", "contained", "mostly", "nicotine", "and", "glycerin.", "The", "following", "compounds", "were", "identified", "from", "the", "ethyl", "acetate", "fraction:", "1.", "2-pentanone,", "4-methyl-", "2.", "butanone,", "2-propoxy-", "3.", "1-pentanol", "4.", "butyl", "acetate", "5.", "3-hexanone", "6.", "2-hexanone", "7.", "isobutyl", "ether", "8.", "3-hexanone,", "5-methyl", "9.", "2-heptanone", "10.", "butyrolactone", "(1,4-butanolide)", "11.", "2-heptanone,", "3-methyl-", "12.", "3-octanone", "13.", "gamma-octalactone", "(buta-4-olide)", "14,", "4-octanone,", "2-methyl-", "15.", "a", "lactone", "(between", "C-8", "and", "C-11)", "16.", "a", "lactone", "(between", "C-8", "and", "C-11),", "possibly", "gamma-caprolactone", "17.", "pyrrolidine,", "1-(l-oxobutyl)-", "18.", "pryidine,", "3-(3,4-dihydro-2H-pyrrol-5yl)-", "(myosmine)", "19.", "1,3-diacetylbenzene", "20.", "benzaldehyde,", "3-ethoxy-2-hydroxy-", "(possibly", "-4-hydroxy)", "282.220TS02"], "bbox": [[76, 59, 97, 67], [130, 59, 147, 67], [162, 59, 180, 67], [195, 59, 280, 69], [77, 87, 126, 96], [151, 86, 205, 96], [216, 86, 290, 98], [304, 86, 335, 96], [347, 86, 399, 95], [412, 86, 474, 96], [76, 116, 126, 125], [152, 115, 238, 125], [250, 115, 276, 126], [294, 115, 334, 125], [77, 144, 157, 156], [172, 144, 258, 156], [270, 144, 290, 153], [304, 144, 378, 153], [389, 144, 547, 156], [572, 144, 657, 156], [670, 144, 690, 154], [703, 145, 778, 154], [789, 144, 809, 154], [76, 158, 129, 168], [141, 158, 246, 170], [76, 187, 107, 197], [120, 187, 215, 199], [226, 187, 269, 197], [281, 188, 365, 199], [379, 189, 431, 199], [444, 187, 604, 197], [616, 187, 701, 197], [714, 187, 734, 199], [77, 202, 215, 212], [226, 202, 246, 212],